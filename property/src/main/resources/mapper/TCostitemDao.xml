    <resultMap type="com.trkj.property.entity.TCostitem" id="TCostitemMap">
        <result property="ctId" column="ct_id" jdbcType="INTEGER"/>
        <result property="residenceId" column="residence_id" jdbcType="INTEGER"/>
        <result property="ctName" column="ct_name" jdbcType="VARCHAR"/>
        <result property="ctChargecycle" column="ct_chargecycle" jdbcType="INTEGER"/>
        <result property="ctSubtotal" column="ct_subtotal" jdbcType="NUMERIC"/>
        <result property="overdue" column="overdue" jdbcType="INTEGER"/>
        <result property="overduefine" column="overduefine" jdbcType="NUMERIC"/>
        <result property="ctRelation" column="ct_relation" jdbcType="VARCHAR"/>
        <result property="chco" column="chco" jdbcType="VARCHAR"/>
        <result property="ifprice" column="ifprice" jdbcType="VARCHAR"/>
        <result property="residenceName" column="residence_name" jdbcType="VARCHAR"/>
        <result property="found" column="found" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="TCostitemCol">
        ct_id
        , residence_id, ct_name, ct_chargecycle, ct_subtotal, overdue, overduefine, ct_relation, chco, ifprice, residence_name, found    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="TCostitemMap">
        select
        <include refid="TCostitemCol"></include>
        from property.t_costitem
        where ct_id = #{ctId}
    </select>

    <!--查询所有-->
    <select id="queryAll" resultMap="TCostitemMap">
        select
        <include refid="TCostitemCol"></include>
        from property.t_costitem
    </select>

    <!--通过实体作为或者条件查询-->
    <select id="queryOrByPojo" resultMap="TCostitemMap">
        select
        <include refid="TCostitemCol"></include>
        from property.t_costitem
        <where>
            <if test="ctId != null">
                or ct_id like CONCAT('%',#{ctId},'%')
            </if>
            <if test="residenceId != null">
                or residence_id like CONCAT('%',#{residenceId},'%')
            </if>
            <if test="ctName != null and ctName != ''">
                or ct_name like CONCAT('%',#{ctName},'%')
            </if>
            <if test="ctChargecycle != null">
                or ct_chargecycle like CONCAT('%',#{ctChargecycle},'%')
            </if>
            <if test="ctSubtotal != null">
                or ct_subtotal like CONCAT('%',#{ctSubtotal},'%')
            </if>
            <if test="overdue != null">
