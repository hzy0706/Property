            </if>
            <if test="overduefine != null">
                or overduefine like CONCAT('%',#{overduefine},'%')
            </if>
            <if test="ctRelation != null and ctRelation != ''">
                or ct_relation like CONCAT('%',#{ctRelation},'%')
            </if>
            <if test="chco != null and chco != ''">
                or chco like CONCAT('%',#{chco},'%')
            </if>
            <if test="ifprice != null and ifprice != ''">
                or ifprice like CONCAT('%',#{ifprice},'%')
            </if>
            <if test="residenceName != null and residenceName != ''">
                or residence_name like CONCAT('%',#{residenceName},'%')
            </if>
            <if test="found != null and found != ''">
                or found like CONCAT('%',#{found},'%')
            </if>
        </where>
    </select>

