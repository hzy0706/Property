<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trkj.property.dao.TCostitemDao">
  <resultMap id="TCostitemMap" type="com.trkj.property.entity.TCostitem">
    <id column="ct_id" jdbcType="INTEGER" property="ctId" />
    <result column="residence_id" jdbcType="INTEGER" property="residenceId" />
    <result column="ct_name" jdbcType="VARCHAR" property="ctName" />
    <result column="ct_chargecycle" jdbcType="INTEGER" property="ctChargecycle" />
    <result column="ct_subtotal" jdbcType="DECIMAL" property="ctSubtotal" />
    <result column="overdue" jdbcType="INTEGER" property="overdue" />
    <result column="overduefine" jdbcType="VARCHAR" property="overduefine" />
    <result column="ct_relation" jdbcType="VARCHAR" property="ctRelation" />
    <result column="chco" jdbcType="VARCHAR" property="chco" />
    <result column="ifprice" jdbcType="CHAR" property="ifprice" />
  </resultMap>
  <sql id="TCostitem">
    ct_id, residence_id, ct_name, ct_chargecycle, ct_subtotal, overdue, overduefine,
    ct_relation, chco, ifprice
  </sql>
  <select id="queryAll" resultMap="TCostitemMap">
    select
    <include refid="TCostitem"></include>
    from property.t_costitem
  </select>
</mapper>