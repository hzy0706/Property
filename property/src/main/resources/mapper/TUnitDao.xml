<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trkj.property.dao.TUnitDao">
  <resultMap id="BaseResultMap" type="com.trkj.property.entity.TUnit">
    <id column="unit_id" jdbcType="INTEGER" property="unitId" />
    <result column="building_id" jdbcType="INTEGER" property="buildingId" />
    <result column="unit_name" jdbcType="VARCHAR" property="unitName" />
    <result column="start_storey" jdbcType="INTEGER" property="startStorey" />
    <result column="end_storey" jdbcType="INTEGER" property="endStorey" />
  </resultMap>
  <sql id="Base_Column_List">
    unit_id, building_id, unit_name, start_storey, end_storey
  </sql>
  <select id="selectByTUnitKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from t_unit
    where unit_id = #{unitId,jdbcType=INTEGER}
  </select>
  <select id="selectAllTUnitByBid" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_unit
    where building_id = #{buildingId,jdbcType=INTEGER}
  </select>
</mapper>