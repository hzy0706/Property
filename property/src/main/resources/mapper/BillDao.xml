<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trkj.property.dao.BillDao">
    <insert id="addBillByCharge" parameterType="com.trkj.property.entity.TBill">
        insert into t_bill values(
                                  #{billId},
                                  #{billDetailId},
                                  #{billChargetime},
                                  #{billState},
                                  #{billRealmoney},
                                  #{billType},
                                  #{billReceiptnumber},
                                  #{billInvoicenumber},
                                  #{billName},
                                  #{billCause},
                                  #{billModifier},
                                  #{billModififerdate}
                                 )

    </insert>

</mapper>