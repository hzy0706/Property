<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trkj.property.dao.TGongtangDao">

    <resultMap type="com.trkj.property.entity.TGongtang" id="TGongtangMap">
        <result property="gId" column="g_id" jdbcType="INTEGER"/>
        <result property="gName" column="g_name" jdbcType="VARCHAR"/>
        <result property="gChargecycle" column="g_chargecycle" jdbcType="INTEGER"/>
        <result property="gSubtotal" column="g_subtotal" jdbcType="NUMERIC"/>
        <result property="chco" column="chco" jdbcType="VARCHAR"/>
        <result property="residenceName" column="residence_name" jdbcType="VARCHAR"/>
        <result property="found" column="found" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="TGongtangCol">
        g_id
        , g_name, g_chargecycle, g_subtotal, chco, residence_name, found    </sql>

    <!--查询单个-->
    <select id="queryById" resultMap="TGongtangMap">
        select
        <include refid="TGongtangCol"></include>
        from property.t_gongtang
        where g_id = #{gId}
    </select>

    <!--查询所有-->
    <select id="queryAll" resultMap="TGongtangMap">
        select
        <include refid="TGongtangCol"></include>
        from property.t_gongtang
    </select>

    <!--通过实体作为或者条件查询-->
    <select id="queryOrByPojo" resultMap="TGongtangMap">
        select
        <include refid="TGongtangCol"></include>
        from property.t_gongtang
        <where>
            <if test="gId != null">
                or g_id like CONCAT('%',#{gId},'%')
            </if>
            <if test="gName != null and gName != ''">
                or g_name like CONCAT('%',#{gName},'%')
            </if>
            <if test="gChargecycle != null">
                or g_chargecycle like CONCAT('%',#{gChargecycle},'%')
            </if>
            <if test="gSubtotal != null">
                or g_subtotal like CONCAT('%',#{gSubtotal},'%')
            </if>
            <if test="chco != null and chco != ''">
                or chco like CONCAT('%',#{chco},'%')
            </if>
            <if test="residenceName != null and residenceName != ''">
                or residence_name like CONCAT('%',#{residenceName},'%')
            </if>
            <if test="found != null and found != ''">
                or found like CONCAT('%',#{found},'%')
            </if>
        </where>
    </select>

    <!--通过实体作为并且条件查询-->
    <select id="queryAndByPojo" resultMap="TGongtangMap">
        select
        <include refid="TGongtangCol"></include>
        from property.t_gongtang
        <where>
            <if test="gId != null">
                and g_id = #{gId}
            </if>
            <if test="gName != null and gName != ''">
                and g_name = #{gName}
            </if>
            <if test="gChargecycle != null">
                and g_chargecycle = #{gChargecycle}
            </if>
            <if test="gSubtotal != null">
                and g_subtotal = #{gSubtotal}
            </if>
            <if test="chco != null and chco != ''">
                and chco = #{chco}
            </if>
            <if test="residenceName != null and residenceName != ''">
                and residence_name = #{residenceName}
            </if>
            <if test="found != null and found != ''">
                and found = #{found}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert">
        insert into property.t_gongtang
        <trim prefix="(" suffix=")" suffixOverrides=",">

            <if test="gName != null">
                g_name,
            </if>
            <if test="gChargecycle != null">
                g_chargecycle,
            </if>
            <if test="gSubtotal != null">
                g_subtotal,
            </if>
            <if test="chco != null">
                chco,
            </if>
            <if test="residenceName != null">
                residence_name,
            </if>
            <if test="found != null">
                found,
            </if>
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">

            <if test="gName != null">
                #{gName},
            </if>
            <if test="gChargecycle != null">
                #{gChargecycle},
            </if>
            <if test="gSubtotal != null">
                #{gSubtotal},
            </if>
            <if test="chco != null">
                #{chco},
            </if>
            <if test="residenceName != null">
                #{residenceName},
            </if>
            <if test="found != null">
                #{found},
            </if>
        </trim>

    </insert>

    <!--批量新增数据 -->
    <insert id="insertBatch">
        <foreach collection="list" item="entity" open="begin" separator=";" close=";end;">
            insert into property.t_gongtang
            <trim prefix="(" suffix=")" suffixOverrides=",">

                <if test="entity.gName != null">
                    g_name,
                </if>
                <if test="entity.gChargecycle != null">
                    g_chargecycle,
                </if>
                <if test="entity.gSubtotal != null">
                    g_subtotal,
                </if>
                <if test="entity.chco != null">
                    chco,
                </if>
                <if test="entity.residenceName != null">
                    residence_name,
                </if>
                <if test="entity.found != null">
                    found,
                </if>
            </trim>
            values
            <trim prefix="(" suffix=")" suffixOverrides=",">

                <if test="entity.gName != null">
                    #{entity.gName},
                </if>
                <if test="entity.gChargecycle != null">
                    #{entity.gChargecycle},
                </if>
                <if test="entity.gSubtotal != null">
                    #{entity.gSubtotal},
                </if>
                <if test="entity.chco != null">
                    #{entity.chco},
                </if>
                <if test="entity.residenceName != null">
                    #{entity.residenceName},
                </if>
                <if test="entity.found != null">
                    #{entity.found},
                </if>
            </trim>
        </foreach>
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update property.t_gongtang
        <set>
            <if test="gName != null">
                g_name = #{gName},
            </if>
            <if test="gChargecycle != null">
                g_chargecycle = #{gChargecycle},
            </if>
            <if test="gSubtotal != null">
                g_subtotal = #{gSubtotal},
            </if>
            <if test="chco != null">
                chco = #{chco},
            </if>
            <if test="residenceName != null">
                residence_name = #{residenceName},
            </if>
            <if test="found != null">
                found = #{found},
            </if>
        </set>
        where g_id = #{gId}
    </update>

    <!--批量修改数据-->
    <update id="updateBatch">
        <foreach collection="list" item="entity" open="begin" separator=";" close=";end;">
            update property.t_gongtang
            <set>
                <if test="entity.gName != null">
                    g_name = #{entity.gName},
                </if>
                <if test="entity.gChargecycle != null">
                    g_chargecycle = #{entity.gChargecycle},
                </if>
                <if test="entity.gSubtotal != null">
                    g_subtotal = #{entity.gSubtotal},
                </if>
                <if test="entity.chco != null">
                    chco = #{entity.chco},
                </if>
                <if test="entity.residenceName != null">
                    residence_name = #{entity.residenceName},
                </if>
                <if test="entity.found != null">
                    found = #{entity.found},
                </if>
            </set>
            where g_id = #{entity.gId}
        </foreach>
    </update>

    <!--通过主键删除数据-->
    <delete id="deleteById">
        delete
        from property.t_gongtang
        where g_id = #{gId}
    </delete>

    <!--通过主键列表批量删除数据-->
    <delete id="deleteBatch">
        delete from property.t_gongtang where g_id in
        <foreach collection="list" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <!--通过实体列表批量删除数据-->
    <delete id="deleteBatchByEntities">
        delete from property.t_gongtang where g_id in
        <foreach collection="list" item="entity" open="(" separator="," close=")">
            #{entity.gId}
        </foreach>
    </delete>
</mapper>

