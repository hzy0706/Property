<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trkj.property.dao.TInspectionroomDao">
  <resultMap id="BaseResultMap" type="com.trkj.property.entity.TInspectionroom">
    <id column="ir_id" jdbcType="INTEGER" property="irId" />
    <result column="house_name" jdbcType="VARCHAR" property="houseName" />
    <result column="house_id" jdbcType="INTEGER" property="houseId" />
    <result column="OkCount" jdbcType="INTEGER" property="okcount" />
    <result column="NoCount" jdbcType="INTEGER" property="nocount" />
  </resultMap>
  <sql id="Base_Column_List">
    ir_id, house_name, house_id, OkCount, NoCount
  </sql>
  <select id="selectByTInspectionroomKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from t_inspectionroom
    where ir_id = #{irId,jdbcType=INTEGER}
  </select>

  <select id="selectAllTInspectionroomByRoomName" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_inspectionroom
    where house_name like #{value}
  </select>

  <select id="selectTInspectionroomByHourseid" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_inspectionroom
    where house_id = #{hourseid}
  </select>

  <insert id="addTInspectionroom" keyColumn="ir_id" keyProperty="irId" parameterType="com.trkj.property.entity.TInspectionroom" useGeneratedKeys="true">
    insert into t_inspectionroom (house_name, house_id, OkCount, 
      NoCount)
    values (#{houseName,jdbcType=VARCHAR}, #{houseId,jdbcType=INTEGER}, #{okcount,jdbcType=INTEGER}, 
      #{nocount,jdbcType=INTEGER})
  </insert>
  <update id="updateByTInspectionroomKeySelective" parameterType="com.trkj.property.entity.TInspectionroom">
    update t_inspectionroom
    <set>
      <if test="houseName != null">
        house_name = #{houseName,jdbcType=VARCHAR},
      </if>
      <if test="houseId != null">
        house_id = #{houseId,jdbcType=INTEGER},
      </if>
      <if test="okcount != null">
        OkCount = #{okcount,jdbcType=INTEGER},
      </if>
      <if test="nocount != null">
        NoCount = #{nocount,jdbcType=INTEGER},
      </if>
    </set>
    where ir_id = #{irId,jdbcType=INTEGER}
  </update>
</mapper>